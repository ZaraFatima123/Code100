public class Mirror_inverse {
    public static void main(String[] args) {
        int count = 0;
        int a[] ={3,4,5,0,1,2};
        for(int i=0;i<a.length;i++){
            if(a[a[i]] == i){
                count++;
            }
        }
        if(count!=0){
            System.out.println("Given array is mirror inverse");
        }
        else{
            System.out.println("Not mirror inverse array");
        }
    }
    
}
