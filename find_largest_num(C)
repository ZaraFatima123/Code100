#include<stdio.h>

int main(){
  int a,b,c;
  
  printf("Enter the value of three numbers a ,b and c: ");
  scanf("%d %d %d",&a,&b,&c);
  
  if(a>b && a>c){
  	printf("a %d is greatest",a);
  }
  else if(b>c){
    printf("b is greatest %d", b);
    }
   else{
      printf("c is greatest %d", c);
     }
  
}
